<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
  logicalFilePath="changelog-4.13">

  <changeSet author="author" id="changelog-4.13">
<preConditions onFail="MARK_RAN">
            <not><columnExists tableName="tc_events" columnName="eventtime"/></not>
        </preConditions>      
    
  <addColumn tableName="tc_events">
      <column name="eventtime" type="TIMESTAMP" />
    </addColumn>
    

  </changeSet>

  <changeSet author="author" id="changelog-4.13-unistall-device">
<preConditions onFail="MARK_RAN">
            <not><columnExists tableName="tc_devices" columnName="dateuninstall"/></not>
        </preConditions>      
    
  <addColumn tableName="tc_devices">
      <column name="dateuninstall" type="VARCHAR(10)" />
    </addColumn>
    

  </changeSet>

</databaseChangeLog>
