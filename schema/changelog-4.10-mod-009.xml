<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
  xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
  xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
  xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.4.xsd"
  logicalFilePath="changelog-4.10-mod-009">


 <changeSet author="author" id="changelog-4.10-mod-device-fuelconsumption">   
	<preConditions onFail="MARK_RAN">
            <not><columnExists tableName="tc_devices" columnName="fuelconsumption"/></not>
        </preConditions>      

    <addColumn tableName="tc_devices">
      <column name="fuelconsumption" type="VARCHAR(128)" defaultValue="0"/>
    </addColumn>


 </changeSet>

</databaseChangeLog>
